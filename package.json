  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@nuxt/kit": "^3.8.2",
    "@rollup/plugin-graphql": "^2.0.4",
    "@vue/apollo-composable": "4.0.0-beta.12",
    "@vue/apollo-option": "4.0.0-beta.12",
    "defu": "^6.1.3",
    "destr": "^2.0.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.2",
    "jiti": "^1.21.0",
    "ohash": "^1.1.3"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/ui": "^0.4.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^20.10.4",
    "eslint": "^8.55.0",
    "nuxt": "^3.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "@nuxtjs/apollo": "link:."
  },
  "packageManager": "pnpm@8.10.0"
}
